/*状态背景*/
page {
  color: #323232;
}
view, text {
  line-height: 1;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.gg-status-bar {
  height: 37px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}
.gg-top {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 0 30rpx;
  position: fixed;
  top: 37px;
  left: 0;
  right: 0;
  z-index: 1000;
  background-color: #ffffff;
  padding-bottom: 10px;
  height: 30px;
  color: #2a2a2a;
}
.gg-top .add-new {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    text-align: right;
    font-size: 12px;
    padding: 10px 0;
    margin: -10px 0;
}
.gg-top .iconfont + view {
    padding-left: 20rpx;
}
.gg-top.between {
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
}
.gg-top.between view {
      font-size: 21px;
      line-height: 30px;
      color: #2a2a2a;
}
.gg-top.between view.iconfont {
        padding: 0 40rpx;
        margin-right: -40rpx;
        font-size: 18px;
        line-height: 18px;
        margin-top: 6px;
        color: #2a2a2a;
}
.gg-top.triple {
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
}
.gg-top.triple view {
      font-size: 21px;
      line-height: 30px;
      color: #2a2a2a;
}
.gg-top.triple view.iconfont {
        font-size: 18px;
        line-height: 18px;
        margin-top: 6px;
        color: #2a2a2a;
        padding: 10px;
        margin: -4px -10px -10px;
}
.gg-top.triple .hide {
      visibility: hidden;
}
