<view class="content"><view class="gg-status-bar"></view><view class="gg-top"><view>{{title}}</view><view data-event-opts="{{[['tap',[['clickback',['$event']]]]]}}" class="iconfont icon-cha" bindtap="__e"></view></view><view class="gg-height"><view class="lists"><view class="item"><view class="bottom"><view class="info">{{Attributes.sf_bili+"%"}}</view><text>{{i18n.dayshifang}}</text></view></view><view class="item"><view class="bottom"><view class="info">{{"￥"+Attributes.freeze*Attributes.a_bili}}</view><text>{{i18n.shifang01}}</text></view></view><view class="item"><view class="bottom"><view class="info">{{"￥"+Attributes.freeze*Attributes.ve_bili}}</view><text>{{i18n.shifang02}}</text></view></view></view><block wx:if="{{frezze}}"><view class="lists"><view class="item"><view class="bottom"><view class="info">{{"￥ "+frezze}}</view><text>{{i18n.AlreadyInvested}}</text></view></view><view class="item"></view><view class="item"></view></view></block></view><view class="pop"><view class="content"><view class="title">{{i18n.InputCurrency}}</view><view class="item"><view class="label">{{''+i18n.SelectCurrency+''}}</view><view class="coins">{{'￥ '+Attributes.freeze+" ≈ "+CurrentQuantity+' RETC'}}</view></view><view class="item trd"><view class="top">{{''+i18n.transactionPassword+''}}</view><view class="bottom"><input type="password" placeholder="{{i18n.transactionPassword}}" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','pwd','$event',[]]]]]]}}" value="{{pwd}}" bindinput="__e"/></view></view><block wx:if="{{!isFirst}}"><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="confirm-btn" bindtap="__e">{{''+(frezze?i18n.Input:i18n.Repeat)}}</view></block></view></view><block wx:if="{{isShowOuterLoading}}"><view><loading vue-id="1" bind:__l="__l"></loading></view></block></view>