<view class="app-setting"><view class="gg-height"></view><view class="gg-status-bar"></view><view data-event-opts="{{[['tap',[['clickClose',['$event']]]]]}}" class="gg-top" bindtap="__e"><view class="iconfont icon-arrow-left"></view><view>{{i18n.sys_set}}</view></view><view class="setting-list"><view class="setting-item" hover-class="list-hover" data-event-opts="{{[['tap',[['toLang',['$event']]]]]}}" bindtap="__e"><text>{{i18n.switch_lang}}</text><text class="right iconfont icon-arrow-right"></text></view><view class="setting-item" hover-class="list-hover" data-event-opts="{{[['tap',[['toprotocol',['$event']]]]]}}" bindtap="__e"><text>{{i18n.protocol}}</text><text class="right iconfont icon-arrow-right"></text></view><view class="setting-item"><text>{{i18n.version}}</text><text class="right r">{{version}}</text></view><view class="exit" hover-class="hover-class" data-event-opts="{{[['tap',[['exit',['$event']]]]]}}" bindtap="__e">{{''+i18n.exit+''}}</view></view><block wx:if="{{showSelectLang}}"><view class="select-lang"><view class="lang-wrapper"><view class="title">{{i18n.select_lang}}</view><view class="langs"><view data-event-opts="{{[['tap',[['selectLang',['chs']]]]]}}" class="{{['lang',(currentLang=='chs')?'active':'']}}" bindtap="__e">{{i18n.zh}}</view><view data-event-opts="{{[['tap',[['selectLang',['en']]]]]}}" class="{{['lang',(currentLang=='en')?'active':'']}}" bindtap="__e">{{i18n.en}}</view><view data-event-opts="{{[['tap',[['selectLang',['ko']]]]]}}" class="{{['lang',(currentLang=='ko')?'active':'']}}" bindtap="__e">{{i18n.ko}}</view></view><view class="lang-btns"><view data-event-opts="{{[['tap',[['cancle',['$event']]]]]}}" bindtap="__e">{{i18n.cancel}}</view><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">{{i18n.confirm}}</view></view></view></view></block></view>