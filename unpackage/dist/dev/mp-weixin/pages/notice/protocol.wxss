.app-setting {
  overflow: hidden;
}
.app-setting .gg-status-bar {
    background-color: #fff;
}
.app-setting .list {
    padding: 0 30rpx 40px;
}
.app-setting .list ._h2 {
      text-align: center;
      font-size: 40rpx;
      line-height: 80rpx;
      font-weight: bold;
}
.app-setting .list ._h4 {
      font-size: 30rpx;
      line-height: 60rpx;
      font-weight: 600;
      color: #666;
}
.app-setting .list ._div {
      font-size: 26rpx;
      line-height: 50rpx;
      color: #929292;
}
.app-setting .list ._div ._p {
        font-size: 26rpx;
}
