<view class="app-history"><view class="gg-height"></view><view class="gg-status-bar"></view><view data-event-opts="{{[['tap',[['clickCBack',['$event']]]]]}}" class="gg-top" bindtap="__e"><view class="iconfont icon-arrow-left"></view><view>{{i18n.transfer_detail}}</view></view><view class="list"><view class="item" hover-class="list-hover"><view class="left"><view class="name">{{list.title+i18n.transfer}}</view><view class="address">{{i18n.OutgoingAddress+"："+list.from}}</view><view class="address">{{i18n.ReceivingAddress+"："+list.to}}</view><view class="time">{{list.create_time}}</view></view><view class="right"><block wx:if="{{list.status==1}}"><view class="state">{{i18n.transfer+" | "+i18n.AuditInProgress}}</view></block><block wx:if="{{list.status==3}}"><view class="state">{{i18n.transfer+" | "+i18n.Adopted}}</view></block><block wx:if="{{list.status==4}}"><view class="state">{{i18n.transfer+" | "+i18n.Rejected}}</view></block><view class="num">{{list.number}}</view><view class="fee">{{i18n.transferfee+"："+list.sfee+list.title}}</view></view></view></view></view>