<view class="app-register"><view class="gg-height"></view><view class="gg-status-bar"></view><view class="gg-top between"><view class="left">{{i18n.hello}}<text>{{i18n.welcome}}</text></view><view data-event-opts="{{[['tap',[['clickClose',['$event']]]]]}}" class="iconfont icon-cha" bindtap="__e"></view></view><view class="tabs"><tabs vue-id="1" tabs="{{tabs}}" data-event-opts="{{[['^changeIdx',[['changeTabIndex']]]]}}" bind:changeIdx="__e" bind:__l="__l"></tabs></view><view class="content"><block wx:if="{{isPhone}}"><view data-event-opts="{{[['tap',[['toArea',['$event']]]]]}}" class="code item" bindtap="__e"><text class="area">{{"+"+areaCode}}</text><view class="iconfont icon-arrow-right"></view></view></block><view class="phone item"><input type="text" placeholder="{{nametip}}" data-event-opts="{{[['input',[['__set_model',['','phoneNumer','$event',[]]]]]]}}" value="{{phoneNumer}}" bindinput="__e"/></view><view class="yzm item"><input type="text" placeholder="{{i18n.enter_yzm}}" data-event-opts="{{[['input',[['__set_model',['','yzm','$event',[]]]]]]}}" value="{{yzm}}" bindinput="__e"/><view class="yzm-wrapper"><yzm vue-id="2" phoneNumer="{{phoneNumer}}" type="{{type}}" isPhone="{{isPhone}}" bind:__l="__l"></yzm></view></view><view class="password item"><block wx:if="{{!isOpen}}"><input type="text" placeholder="{{i18n.enter_login_pass}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></block><block wx:if="{{isOpen}}"><input type="password" placeholder="{{i18n.enter_login_pass}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></block><view data-event-opts="{{[['tap',[['eye',[0]]]]]}}" class="{{['iconfont',(!isOpen)?'icon-yanjing1':'',(isOpen)?'icon-yanjing2':'']}}" bindtap="__e"></view></view><view class="repassword item"><block wx:if="{{!isOpen1}}"><input type="text" placeholder="{{i18n.reenter_login_pass}}" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/></block><block wx:if="{{isOpen1}}"><input type="password" placeholder="{{i18n.reenter_login_pass}}" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/></block><view data-event-opts="{{[['tap',[['eye',[1]]]]]}}" class="{{['iconfont',(!isOpen1)?'icon-yanjing1':'',(isOpen1)?'icon-yanjing2':'']}}" bindtap="__e"></view></view><view class="item imgyzm"><input type="text" placeholder="{{i18n.enter_imgyzm}}" data-event-opts="{{[['input',[['__set_model',['','yzmImg','$event',[]]]]]]}}" value="{{yzmImg}}" bindinput="__e"/><view class="yzm-img"><image src="{{captcha}}" data-event-opts="{{[['tap',[['updatecaptcha',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="refer item"><input type="text" placeholder="{{i18n.enter_refer}}" data-event-opts="{{[['input',[['__set_model',['','refer','$event',[]]]]]]}}" value="{{refer}}" bindinput="__e"/></view><view class="protocol item"><image src="{{isProtocol1?'/static/imgs/protocol1.png':'/static/imgs/protocol2.png'}}" data-event-opts="{{[['tap',[['clickprotocol',['$event']]]]]}}" bindtap="__e"></image>{{''+i18n.i}}<text data-event-opts="{{[['tap',[['clickagreeprotocol',['$event']]]]]}}" bindtap="__e">{{i18n.iagree}}</text></view><view class="register" hover-class="hover-class" data-event-opts="{{[['tap',[['doRegister',['$event']]]]]}}" bindtap="__e">{{i18n.register}}</view><view data-event-opts="{{[['tap',[['tologin',['$event']]]]]}}" class="login-text" bindtap="__e">{{''+i18n.haveAccount+''}}<text class="tologin">{{i18n.login}}</text></view></view></view>