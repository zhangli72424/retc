.app-register .content {
  padding: 0 30rpx;
}
.app-register .content .item {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    margin-top: 56rpx;
}
.app-register .content .item .label {
      font-size: 14px;
      color: #929292;
}
.app-register .content .item .bottom {
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
          -ms-flex-pack: justify;
              justify-content: space-between;
      height: 80rpx;
      -webkit-box-align: center;
      -webkit-align-items: center;
          -ms-flex-align: center;
              align-items: center;
      border: 1rpx solid #e2e2e2;
      margin-top: 30rpx;
      border-radius: 10rpx;
      padding: 0 30rpx;
}
.app-register .content .item .bottom view:first-child {
        font-size: 14px;
        color: #2a2a2a;
}
.app-register .content .item .bottom .copy {
        font-size: 14px;
        color: #1aafaf;
        padding: 10px;
        margin: -10px;
        width: 100rpx;
}
.app-register .content .item .bottom .ttt {
        font-size: 10px;
        padding-right: 20rpx;
        line-height: 12px;
        word-break: break-all;
        -webkit-box-flex: 1;
        -webkit-flex: 1;
            -ms-flex: 1;
                flex: 1;
}
.app-register .content .generatebtn {
    width: 360rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    margin: 100rpx auto 60rpx;
    font-size: 14px;
    color: #ffffff;
    border-radius: 10rpx;
    background-color: #1aafaf;
}

