<view class="app-bill"><view class="gg-height"></view><view class="gg-status-bar"></view><view class="gg-top"><view data-event-opts="{{[['tap',[['clickClose',['$event']]]]]}}" class="iconfont icon-arrow-left" bindtap="__e"></view><view data-event-opts="{{[['tap',[['clickClose',['$event']]]]]}}" bindtap="__e">{{i18n.mybill}}</view></view><view class="gg-line"></view><block wx:if="{{tabs.length>0}}"><view class="content"><tabs vue-id="1" tabs="{{tabs}}" space="1" data-event-opts="{{[['^handleClick',[['handleClick']]]]}}" bind:handleClick="__e" bind:__l="__l"></tabs></view></block><view class="uni-total-flex"><view class="txt"><text>{{i18n.total_income+":"}}</text><text>{{total}}</text></view></view><view class="split"></view><view class="list"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="left"><view class="name">{{item.typeStr}}</view><view class="time">{{item.time}}</view></view><view class="num"><view class="name">{{item.num+"("+item.pcs+")"}}</view><view class="time">{{item.amount+" CYE"}}</view></view></view></block><block wx:if="{{lists.length==0}}"><view class="nodata">{{''+i18n.nodata+''}}</view></block></view></view>