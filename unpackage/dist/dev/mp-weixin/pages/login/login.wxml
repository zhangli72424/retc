<view class="app-register"><view class="gg-height"></view><view class="gg-status-bar"></view><view class="gg-top between"><view class="left">{{i18n.hello}}<text>{{i18n.welcome}}</text></view></view><view class="content"><view class="phone item"><input type="text" placeholder="{{i18n.enter_num_phone}}" data-event-opts="{{[['input',[['__set_model',['','phoneNumer','$event',[]]]]]]}}" value="{{phoneNumer}}" bindinput="__e"/></view><view class="password item"><block wx:if="{{!isOpen}}"><input type="text" placeholder="{{i18n.enterpassword_cye}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></block><block wx:if="{{isOpen}}"><input type="password" placeholder="{{i18n.enterpassword_cye}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></block><view data-event-opts="{{[['tap',[['eye',['$event']]]]]}}" class="{{['iconfont',(!isOpen)?'icon-yanjing1':'',(isOpen)?'icon-yanjing2':'']}}" bindtap="__e"></view></view><view class="other-content"><view data-event-opts="{{[['tap',[['toForget',['$event']]]]]}}" class="tip" bindtap="__e">{{i18n.forgetPass}}</view><view data-event-opts="{{[['tap',[['switchLang',['$event']]]]]}}" class="switch_lang" bindtap="__e">{{i18n.switch_lang}}</view></view><view class="{{['login',hassoment?'active':'']}}" hover-class="hover-class" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">{{i18n.login}}</view><view data-event-opts="{{[['tap',[['tologin',['$event']]]]]}}" class="login-text" bindtap="__e">{{''+i18n.notAccount+''}}<text class="tologin">{{i18n.signin}}</text></view></view><block wx:if="{{showSelectLang}}"><view class="select-lang"><view class="lang-wrapper"><view class="title">{{i18n.select_lang}}</view><view class="langs"><view data-event-opts="{{[['tap',[['selectLang',['chs']]]]]}}" class="{{['lang',(currentLang=='chs')?'active':'']}}" bindtap="__e">{{i18n.zh}}</view><view data-event-opts="{{[['tap',[['selectLang',['en']]]]]}}" class="{{['lang',(currentLang=='en')?'active':'']}}" bindtap="__e">{{i18n.en}}</view><view data-event-opts="{{[['tap',[['selectLang',['ko']]]]]}}" class="{{['lang',(currentLang=='ko')?'active':'']}}" bindtap="__e">{{i18n.ko}}</view></view><view class="lang-btns"><view data-event-opts="{{[['tap',[['cancle',['$event']]]]]}}" bindtap="__e">{{i18n.cancel}}</view><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" bindtap="__e">{{i18n.confirm}}</view></view></view></view></block><block wx:if="{{isShowLoading}}"><view><loading vue-id="1" bind:__l="__l"></loading></view></block></view>